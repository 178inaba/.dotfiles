[core]
	editor = emacs
[pager]
	log = diff-highlight | less
	show = diff-highlight | less
	diff = diff-highlight | less
[alias]
	d = !git diff --color | diff-highlight | less -R
	l = !git log --color | diff-highlight | less -R
	s = !git show --color | diff-highlight | less -R
	cc = checkout .
[include]
	path = ~/.gitconfig.local
[diff "xlsx"]
	binary = true
	textconv = git-xlsx-textconv
